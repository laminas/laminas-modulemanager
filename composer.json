{
    "name": "laminas/laminas-modulemanager",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "modulemanager"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-modulemanager/",
        "issues": "https://github.com/laminas/laminas-modulemanager/issues",
        "source": "https://github.com/laminas/laminas-modulemanager",
        "rss": "https://github.com/laminas/laminas-modulemanager/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas/laminas-eventmanager": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-config": "self.version",
        "laminas/laminas-console": "self.version",
        "laminas/laminas-loader": "self.version",
        "laminas/laminas-mvc": "self.version",
        "laminas/laminas-servicemanager": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-config": "Laminas\\Config component",
        "laminas/laminas-console": "Laminas\\Console component",
        "laminas/laminas-loader": "Laminas\\Loader component",
        "laminas/laminas-mvc": "Laminas\\Mvc component",
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ModuleManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ModuleManager\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-modulemanager": "self.version"
    }
}
